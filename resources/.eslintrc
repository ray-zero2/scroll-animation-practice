{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "globals": {
    "velocity": true,
    "YT": true
  },
  "rules": {
    "no-console"  : 0,
    "no-alert"    : 0,
    "no-debugger" : 1,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "bracketSpacing": true
      }
    ]
  }
}
